		.page1 .productos{
			text-align: center;
			margin-top: 5px;
		}
		.page1 .producto {
			display: inline-block;
			margin: 0 -40px;
			transition: all 500ms;
		}
			.page1 .producto:hover{
				transform: scale(1.1);
			}
			.page1 .producto:nth-child(1) img, .page1 .producto:nth-child(9) img{
				width: 70%;
			}
			.page1 .producto:nth-child(2) img, .page1 .producto:nth-child(8) img{
				width: 80%;
				transition-delay: 200ms;
			}
			.page1 .producto:nth-child(3) img, .page1 .producto:nth-child(7) img{
				width: 90%;
				transition-delay: 400ms;
			}
			.page1 .producto:nth-child(4) img, .page1 .producto:nth-child(6) img{
				width: 100%;
				transition-delay: 600ms;
			}
			.page1 .producto:nth-child(5){
				z-index: 4;
			}
			.page1 .producto:nth-child(6){
				z-index: 3;
			}
			.page1 .producto:nth-child(7){
				z-index: 2;
			}
			.page1 .producto:nth-child(8){
				z-index: 1;
			}
			.page1 .producto img{
				width: 110%;
			}
	

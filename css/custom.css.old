	body#ultra-premium .single-item{
		/*position: fixed;*/
		position: relative;
		float: left;
		width: 50%;
		height: 100%;
		top: 0;
		right: 0;
	}
		body#ultra-premium .single-item .item{
			position: relative;
			float: left;
			width: 100%;
			/*height: 70%;
			padding-top: 15%;
			padding-bottom: 15%;*/
			text-align: center;
			overflow: hidden;
			background-color: #fff;
			z-index: 2;
		}
		body#ultra-premium .single-item .item h3{
			position: absolute;
			width: 50%;
			margin: 0 25%;
			font-family: 'Bebas Neue';
			font-size: 300px;
			color: #eaeaea;
			
			transform: translate(0, -500px);
			filter: blur(10px);
			transition: all 600ms;
		}
		body#ultra-premium .single-item .item p{
			width: 50%;
			margin: 0 25%;
			font-family: 'Bebas Neue';
			font-size: 30px;
			font-weight: normal;
			color: #231f20;
			margin-top: 150px;
			z-index: 2;
			position: relative;
			
			transform: translate(0, 500px);
			filter: blur(10px);
			transition: all 800ms;
		}
			
			body#ultra-premium .single-item.slick-active p, body#ultra-premium .single-item.slick-active h3{
				transform: translate(0);
				filter: blur(0);
			}
			body#ultra-premium .single-item.slick-active .icons p{
				transform: scale(1);
				filter: blur(0);
			}
		
		body#ultra-premium .single-item .icons{
			margin-top: 200px;
		}
			body#ultra-premium .single-item .icons p{
				float: left;
				font-size: 13px;
				margin: 0;
				text-align: center;
				
				transform: scale(0.1);
				filter: blur(10px);
				transition: all 500ms;
				transition-delay: 1000ms;
			}
			body#ultra-premium .single-item .icons img{
				display: inline-block;
				width: 20px;
			}
			body#ultra-premium .tabla{
				max-width: 75%;
				margin-top: 25px;
			}
		body#ultra-premium .slick-dots{
			position: fixed;
			left: 45%;
			bottom: 50%;
			margin: 0;
			transform: rotate(90deg);
		}
			body#ultra-premium .slick-dots li button{
				opacity: 0.3;
			}
			body#ultra-premium .slick-dots li.slick-active button{
				opacity: 1;
			}
		body#ultra-premium table{
			float: left;
			width: 70%;
			margin-left: 50px;
			margin-top: 25px;
		}
			body#ultra-premium table tr{
				font-family: 'Frutiger LT Std';
				font-weight: 600;
				border-bottom: 2px solid #a1a1a0;
				
				transform: translate(0, 500px);
				filter: blur(10px);
				transition: all 600ms;
			}
				body#ultra-premium .slick-active table tr{
					transform: translate(0);
					filter: blur(0);
				}
				
				body#ultra-premium table tr:nth-child(2){
					transition-delay: 200ms;
				}
				body#ultra-premium table tr:nth-child(3){

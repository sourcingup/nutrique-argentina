@font-face {
    font-family: 'Bebas Neue';
    src: url('../fonts/subset-BebasNeueBold.woff2') format('woff2'),
        url('../fonts/subset-BebasNeueBold.woff') format('woff');
    font-weight: bold;
    font-style: normal;
}

@font-face {
    font-family: 'Bebas Neue';
    src: url('../fonts/subset-BebasNeueRegular.woff2') format('woff2'),
        url('../fonts/subset-BebasNeueRegular.woff') format('woff');
    font-weight: normal;
    font-style: normal;
}

@font-face {
    font-family: 'Frutiger LT Std';
    src: url('../fonts/subset-FrutigerLTStd-Light.woff2') format('woff2'),
        url('../fonts/subset-FrutigerLTStd-Light.woff') format('woff');
    font-weight: 300;
    font-style: normal;
}

html{
  height: 100%;
}
a{
	outline: none;
}
	body{
		padding: 0;
		text-align: center;
		position: relative;
		margin: 0;
		height: 100%;
		-webkit-font-smoothing: antialiased;
		font-family: 'Frutiger LT Std';
	}
	
	.wow {
	  visibility: hidden;
	}
	
	.wrapper {
		height: 100% !important;
		height: 100%;
		margin: 0 auto; 
		overflow: hidden;
	}
	
	header{
		position: fixed !important;
		/*width: 90%;
		padding: 0 5%;*/
		margin-top: 25px;
		z-index: 12;
	}
		header #logo{
			float: left;
		}
			header #logo img{
				width: 100%;
				margin: 0;
			}
		header #menuButton{
			position: relative;
			float: right;
			padding: 2px 8px;
			background-color: #000;
			font-size: 22px;
			color: #fff;
			z-index: 4;
		}
	
	header #menu{
		position: fixed;
		height: 100%;
		background-color: #FFF;
		top: 0;
		right: 0;
		z-index: 3;
	}
		header #menu #close{
			position: relative;
			float: left;
			padding: 2px 8px;
			background-color: #000;
			font-size: 22px;
			color: #fff;
			z-index: 4;
			margin-top: 10px;
		}
		header #menu #logo-menu{
			float:left;
			clear: left;
			margin-top: 10px;
		}
			header #menu #logo-menu img{
				width: 100%;
			}
		header #menu h3 a{
			float:left;
			width: 80%;
			font-family: 'Bebas Neue';
			font-size: 25px;
			color: #1d1d1b;
			text-align: center;
			margin: 20px 10%;
			text-decoration: none;
		}
		header #menu ul{
			float: left;
			width: 80%;
			padding: 0;
			list-style: none;
			margin: 0 10%;
			text-align: center;
		}
			header #menu ul li a{
				display: inline-block;
				padding: 8px 0;
				font-family: 'Bebas Neue';
				font-size: 22px;
				color: #1d1d1b;
				line-height: 25px;
				text-decoration: none;
			}
				header #menu ul li.nosotros a, header #menu ul li.contacto a{
					color: #6f7073;
				}
		header #ig{
			display: inline-block;
			text-align: center;
		}
			header #ig img{
				width: 100%;
			}
	
	.item{
		float: left;
	}
	.slick-slide{
		outline: none;
	}
	.slick-arrow{
		display: none !important;
	}
	#section0{
		float: left;
		width: 100%;
	}
	.section{
		float: left;
		width: 100%;
	}

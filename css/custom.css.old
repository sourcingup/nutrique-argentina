	.slick-dots{
		position: absolute;
		width: 100%;
		/*margin-top: -15px;*/
		bottom: -15px;
		text-align: center;
		padding: 0;
	}
		.slick-dots li{
			display: inline-block;
			margin: 0 10px;
		}
			.slick-dots li button{
				width: 10px;
				height: 10px;
				background-color: #000;
				border-radius: 50px;
				border: 0;
				text-indent: -9999px;
				padding: 0;
				opacity: 0.6;
			}
				.slick-dots li.slick-active button{
					opacity: 1;
				}
	
